<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_move_tree_custom" model="ir.ui.view">
        <field name="name">account.move.tree.custom</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="action_generate_xlsx_report" type="object" class="btn btn-primary"
                        string="Generate Invoice Excel" icon="fa-magic"/>
            </xpath>
        </field>
    </record>
</odoo>